#include <stdio.h>

int main() {int arr[10];

int a =0;

int b = 9;

int num;



    printf("입력:");

for(int i = 0; i<10; i++){

        scanf("%d",&num);

if(num % 2 == 1){

            arr[a] = num;

            a++;

        }else {

            arr[b] = num;

            b--;

        }

    }

for(int i = 0; i<10; i++){

        printf("%d",arr[i]);

    }



return 0;

}
