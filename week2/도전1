#include <stdio.h>



void Odd(int* num){

for(int i = 0; i<10; i++){

if(i % 2 == 0)

            printf("%d ",num[i]);

    }

}



void Even(int* num){

for(int i = 0; i<10; i++){

if(i % 2 == 1)

            printf("%d ",num[i]);

    }

}



int main() {

int arr[10];



    printf("입력:");

for(int i = 0; i<10; i++)

        scanf("%d",&arr[i]);



    Odd(arr);

    printf("\n");

    Even(arr);



return 0;

}
