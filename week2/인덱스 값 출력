#include <stdio.h>

int main() {

    int arr[10];
    int idx = 0;
     for(int i=0; i<10; i++){
             arr[i] = i+1;
     }
     for(int i=0; i<10; i++){
         if(i%2 != 0)
             printf("%d ",arr[i]);
     }

    printf("\n");
     
    for(int i=0; i<10; i++){
        if(i%2 != 1)
            printf("%d ",arr[i]);
    }

    return 0;
}










#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    /* int idx  = 0;
    int number=0;
    int upper = 0;
    int lower =0;
    int etc = 0; */

    printf("문자열 입력 :");
    scanf("%s",str);
    int n = strlen(str);

    int num=0, small = 0, big = 0, special = 0;
    for(int i=0; i<n; i++){
        if(str[i]>='A' && str[i]<='Z')
            big++;
            else if(str[i]>='a' && str[i]<='z')
                small++;
            else if(str[i]>='0' && str[i]<='9')
                num++;
            else special++;
    }
    printf("숫자 :%d ",num);
    printf("소문자 :%d ",small);
    printf("대문자 :%d ",big);
    printf("특수문자 :%d ",special);






    /* while(str[idx] != NULL){
        if(str[idx]>='0' && str[idx]<='9')
            number++;
        else if(str[idx]>='a' && str[idx]<='z')
            lower++;
        else if(str[idx]>='A' && str[idx]<='Z')
            upper++;
        else etc++;
        idx++;
    }

    printf("숫자 :%d ",number);
    printf("소문자 :%d ",lower);
    printf("대문자 :%d ",upper);
    printf("특수문자 :%d ",etc);
*/
    return 0;
}
