#include <stdio.h>

int main() {

    int arr[10];
    int idx = 0;
     for(int i=0; i<10; i++){
             arr[i] = i+1;
     }
     for(int i=0; i<10; i++){
         if(i%2 != 0)
             printf("%d ",arr[i]);
     }

    printf("\n");
     
    for(int i=0; i<10; i++){
        if(i%2 != 1)
            printf("%d ",arr[i]);
    }

    return 0;
}
