#include <stdio.h>

int main()
{
    int a,b;
    
    scanf("%d %d",&a,&b);
    
    if(a == 0 && b >= 45){
        printf("%d %d",a,b-45);
    }else if(a == 0 && b < 45){
        printf("%d %d",23,(60+b)-45);
    }else if(a != 0 && b >= 45){
        printf("%d %d",a,b-45);
    }else if(a != 0 && b < 45){
        printf("%d %d",a-1,(60+b)-45);
    }

    return 0;
}
