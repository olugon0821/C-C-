#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct point{
    int x;
    int y;
};

int main() {
    struct point arr[3];

    for (int i = 0; i < 3; ++i) {
        printf("점의 좌표 입력 : ");
        scanf("%d %d", &arr[i].x,&arr[i].y);
    }
    for (int i = 0; i < 3; ++i) {
        printf("[%d, %d]",arr[i].x, arr[i].y);
    }
    return 0;
}
