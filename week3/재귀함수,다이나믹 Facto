#include <stdio.h>
 
 int facto(int num){
   if(num == 0)
       return 1;
   else{
       return num * facto(num-1);
   }
}

int main() {

    int a;

    scanf("%d",&a);

    printf("%d", facto(a));
    return 0;
}


int facto(int a){
    int num = 1;
    for(int i = 1; i <= a ; i++){
        num *= i;
    }
    return num;
}

int main(){
    int a;
    scanf("%d", &a);
    printf("%d", facto(a));
}
