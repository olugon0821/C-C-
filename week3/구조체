#include <stdio.h>
#include <stdlib.h>
#include <string.h>

struct person{
    char name[20];
    char phone[20];
    int age;
};

int main() {
    struct person m1,m2;
    strcpy(m1.name,"안성준");
    strcpy(m1.phone,"010-1122-3344");
    m1.age = 23;

    printf("이름 입력 : ");
    scanf("%s",m2.name);
    printf("번호 입력 : ");
    scanf("%s",m2.phone);
    printf("나이 입력 : ");
    scanf("%d",&(m2.age));

    printf("이름 :%s \n",m1.name);
    printf("번호 :%s \n",m1.phone);
    printf("나이 :%d \n",m1.age);

    printf("이름 :%s \n",m2.name);
    printf("번호 :%s \n",m2.phone);
    printf("나이 :%d \n",m2.age);
    return 0;
}
