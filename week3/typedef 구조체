#include <stdio.h>
struct point{
    int x;
    int y;
};
typedef struct point Point;

typedef struct person{
    char name[20];
    char phone[20];
    int age;
} Person;

int main() {
    Point pos = {10,20};
    Person  man ={"이승기", "010-4546-1231",21};
    printf("%d %d\n", pos.x,pos.y);
    printf("%s %s %d",man.name,man.phone,man.age);
    return 0;
}
